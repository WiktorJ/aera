<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format2.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="2">
    <name>aera_semi_autonomous</name>
    <version>0.1.0</version>
    <description>Package to run AR4 arm using one-shot semi autonomous pipeline</description>

    <license>Apache License 2.0</license>
    <maintainer email="wiktor.jurasz@gmail.com">Wiktor Jurasz</maintainer>
    <author email="wiktor.jurasz@gmail.com">Wiktor Jurasz</author>

    <buildtool_depend>ament_python</buildtool_depend>

<!--    <exec_depend>ar_moveit_config</exec_depend>-->
    <exec_depend>cv_bridge</exec_depend>
    <exec_depend>easy_handeye2</exec_depend>
    <exec_depend>geometry_msgs</exec_depend>
    <exec_depend>rclpy</exec_depend>
    <exec_depend>depthai_ros_driver</exec_depend>
    <exec_depend>ros2_aruco</exec_depend>
    <exec_depend>tf2_ros</exec_depend>
    <exec_depend>sensor_msgs</exec_depend>
    <exec_depend>std_msgs</exec_depend>

<!--    &lt;!&ndash; These test dependencies are optional-->
<!--    Their purpose is to make sure that the code passes the linters &ndash;&gt;-->
    <test_depend>ament_copyright</test_depend>
    <test_depend>ament_flake8</test_depend>
    <test_depend>ament_pep257</test_depend>
    <test_depend>python3-pytest</test_depend>

    <export>
        <build_type>ament_python</build_type>
    </export>
</package>